    <%- include("header") %>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <% for (toDo of toDos) { %>
        <form action="/delete" method="post">
            <div class="item">
                <input onChange="this.form.submit()" type="checkbox" value=<%= toDo.id %> name="checkbox" id="">
                <p><%= toDo.name %></p>
            </div>
            <input type="hidden" name="listName" value=<%= listTitle %>>
        </form>
        <% } %>
        <form class="item" action="/" method="post">
            <input type="text" name="newToDo" placeholder="New ToDo" autocomplete="off">
            <br>
            <button type="submit" name="list" value=<%= listTitle %>>
                +</button>
        </form>
    </div>
    <%- include("footer") %>